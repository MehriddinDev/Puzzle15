<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="false">
    <shape>
        <solid android:color="#0E9285"/>
        <stroke android:width="3dp" android:color="#0E9285"/>
    </shape>
</item>

    <item android:state_pressed="true">
        <shape>
            <solid android:color="#04322D"/>
            <stroke android:width="3dp" android:color="#04322D"/>
        </shape>
    </item>

</selector>